<% if (!logged) { %>
<script>

        socket.on('signInResponse',function(data){
              if(data.success){
                window.location.href = "https://chat.enam.io/#profile";
                location.reload();
              } else
                  alert("Sign in unsuccessul.");
          });
          socket.on('signUpResponse',function(data){
              if(data.success){
                window.location.href = "https://chat.enam.io/#profile";
                location.reload();
              } else
                  alert("Sign up unsuccessul.");
          });
      
      </script>
   <% }  %>
